{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "useTabs": true,
  "importOder": [
	"<THIRD_PARTY_MODULES>",
	"^@/components/(.*)$",
	"^@/screens/(.*)$",
	"^@/ui/(.*)$",
	"^@/hooks/(.*)$",
	"^@/shared/(.*)$",
	"^@/services/(.*)$",
	"^@/assets/(.*)$",
	"^@/utils/(.*)$",
	"^@/config/(.*)$",
	"^@/store/(.*)$",
	"^../(.*)",
	"^./(.*)",
	"(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
